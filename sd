[32m+[m[32mimport Button from "react-bootstrap/Button";[m
[32m+[m[32mimport Modal from "react-bootstrap/Modal";[m
[32m+[m[32mimport { useEffect, useState } from "react";[m
[32m+[m[32mimport { putUpdateUser } from "../../services/UserService";[m
[32m+[m[32mimport { toast } from "react-toastify";[m
[32m+[m[32mimport { postCreateUserRole } from "../../services/UserRoleService";[m
[32m+[m
[32m+[m[32mconst ModalEdit = (props) => {[m
[32m+[m[32m  const { handleClose, show, handleUpdateTable, dataUserEdit, listRoles } =[m
[32m+[m[32m    props;[m
[32m+[m
[32m+[m[32m  const [selectedRole, setSelectedRole] = useState("");[m
[32m+[m[32m  const [name, setName] = useState("");[m
[32m+[m[32m  const [phone, setPhone] = useState("");[m
[32m+[m[32m  const [password, setPassword] = useState("");[m
[32m+[m[32m  const [address, setAddress] = useState("");[m
[32m+[m[32m  const [dateOfBirth, setDateOfBirth] = useState("");[m
[32m+[m[32m  const [selectId, setSelectId] = useState(0);[m
[32m+[m[32m  const roleName =[m
[32m+[m[32m    dataUserEdit.roles && dataUserEdit.roles.length > 0[m
[32m+[m[32m      ? dataUserEdit.roles[0].id[m
[32m+[m[32m      : "";[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    if (show) {[m
[32m+[m[32m      setSelectId(dataUserEdit.id);[m
[32m+[m[32m      setName(dataUserEdit.name);[m
[32m+[m[32m      setPhone(dataUserEdit.phone);[m
[32m+[m[32m      setAddress(dataUserEdit.address);[m
[32m+[m[32m      setSelectedRole(roleName);[m
[32m+[m[32m    }[m
[32m+[m[32m  }, [dataUserEdit, show, roleName]);[m
[32m+[m
[32m+[m[32m  const handleSave = async () => {[m
[32m+[m[32m    let res = await putUpdateUser([m
[32m+[m[32m      selectId,[m
[32m+[m[32m      name,[m
[32m+[m[32m      phone,[m
[32m+[m[32m      password,[m
[32m+[m[32m      dateOfBirth,[m
[32m+[m[32m      address[m
[32m+[m[32m    );[m
[32m+[m[32m    if (res) {[m
[32m+[m[32m      handleClose();[m
[32m+[m[32m      setAddress("");[m
[32m+[m[32m      setDateOfBirth("");[m
[32m+[m[32m      setName("");[m
[32m+[m[32m      setPassword("");[m
[32m+[m[32m      setPhone("");[m
[32m+[m[32m      toast.success("Update successed!");[m
[32m+[m[32m      handlePutUpdateUserRole(res?.id);[m
[32m+[m[32m      handleUpdateTable();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      toast.error("Update false!");[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m[32m  const handlePutUpdateUserRole = async (id) => {[m
[32m+[m[32m    const userId = id;[m
[32m+[m[32m    const roleId = +selectedRole;[m
[32m+[m[32m    let res = await postCreateUserRole(userId, roleId);[m
[32m+[m[32m    if (res && res.id) {[m
[32m+[m[32m      handleClose();[m
[32m+[m[32m    } else {[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleRoleChange = (event) => {[m
[32m+[m[32m    setSelectedRole(event.target.value);[m
[32m+[m[32m  };[m
[32m+[m[32m  return ([m
[32m+[m[32m    <Modal show={show} onHide={handleClose} backdrop="static" keyboard={false}>[m
[32m+[m[32m      <Modal.Header closeButton>[m
[32m+[m[32m        <Modal.Title>Edit user</Modal.Title>[m
[32m+[m[32m      </Modal.Header>[m
[32m+[m[32m      <Modal.Body>[m
[32m+[m[32m        <div className="body-add-new">[m
[32m+[m[32m          <form className="row g-3">[m
[32m+[m[32m            <div className="col-12">[m
[32m+[m[32m              <label className="form-label">Full Name</label>[m
[32m+[m[32m              <input[m
[32m+[m[32m                type="text"[m
[32m+[m[32m                className="form-control"[m
[32m+[m[32m                value={name}[m
[32m+[m[32m                onChange={(e) => setName(e.target.value)}[m
[32m+[m[32m              />[m
[32m+[m[32m            </div>[m
[32m+[m[32m            <div className="col-12">[m
[32m+[m[32m              <label className="form-label">Phone</label>[m
[32m+[m[32m              <input[m
[32m+[m[32m                type="text"[m
[32m+[m[32m                className="form-control"[m
[32m+[m[32m                value={phone}[m
[32m+[m[32m                onChange={(e) => setPhone(e.target.value)}[m
[32m+[m[32m              />[m
[32m+[m[32m            </div>[m
[32m+[m[32m            <div className="col-12">[m
[32m+[m[32m              <label className="form-label">Address</label>[m
[32m+[m[32m              <input[m
[32m+[m[32m                type="text"[m
[32m+[m[32m                className="form-control"[m
[32m+[m[32m                value={address}[m
[32m+[m[32m                onChange={(e) => setAddress(e.target.value)}[m
[32m+[m[32m              />[m
[32m+[m[32m            </div>[m
[32m+[m[32m            <div className="col-12">[m
[32m+[m[32m              <label className="form-label">Password</label>[m
[32m+[m[32m              <input[m
[32m+[m[32m                type="password"[m
[32m+[m[32m                className="form-control"[m
[32m+[m[32m                value={password}[m
[32m+[m[32m                onChange={(e) => setPassword(e.target.value)}[m
[32m+[m[32m              />[m
[32m+[m[32m            </div>[m
[32m+[m[32m            <div className="col-md-6">[m
[32m+[m[32m              <label className="form-label">Date of birth</label>[m
[32m+[m[32m              <input[m
[32m+[m[32m                type="date"[m
[32m+[m[32m                className="form-control"[m
[32m+[m[32m                value={dateOfBirth}[m
[32m+[m[32m                onChange={(e) => setDateOfBirth(e.target.value)}[m
[32m+[m[32m              />[m
[32m+[m[32m            </div>[m
[32m+[m[32m            <div className="col-md-6">[m
[32m+[m[32m              <label className="form-label">Role</label>[m
[32m+[m[32m              <select[m
[32m+[m[32m                className="form-select"[m
[32m+[m[32m                value={selectedRole}[m
[32m+[m[32m                onChange={handleRoleChange}[m
[32m+[m[32m              >[m
[32m+[m[32m                <option value="" disabled></option>[m
[32m+[m[32m                {listRoles.map((state) => ([m
[32m+[m[32m                  <option key={state.id} value={state.id}>[m
[32m+[m[32m                    {state.name}[m
[32m+[m[32m                  </option>[m
[32m+[m[32m                ))}[m
[32m+[m[32m              </select>[m
[32m+[m[32m            </div>[m
[32m+[m[32m          </form>[m
[32m+[m[32m        </div>[m
[32m+[m[32m      </Modal.Body>[m
[32m+[m[32m      <Modal.Footer>[m
[32m+[m[32m        <Button variant="secondary" onClick={handleClose}>[m
[32m+[m[32m          Close[m
[32m+[m[32m        </Button>[m
[32m+[m[32m        <Button variant="primary" onClick={() => handleSave()}>[m
[32m+[m[32m          Save[m
[32m+[m[32m        </Button>[m
[32m+[m[32m      </Modal.Footer>[m
