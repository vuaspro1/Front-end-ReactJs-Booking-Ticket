[32m+[m[32mimport axios from "./axios";[m
[32m+[m
[32m+[m[32mconst fetchAllUser = (page) => {[m
[32m+[m[32m  return axios.get(`/api/User?page=${page}&pageSize=10`);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst postCreateUser = ([m
[32m+[m[32m  name,[m
[32m+[m[32m  phone,[m
[32m+[m[32m  password,[m
[32m+[m[32m  dateOfBirth,[m
[32m+[m[32m  address,[m
[32m+[m[32m  userName[m
[32m+[m[32m) => {[m
[32m+[m[32m  return axios.post("/api/User", {[m
[32m+[m[32m    name,[m
[32m+[m[32m    phone,[m
[32m+[m[32m    password,[m
[32m+[m[32m    dateOfBirth,[m
[32m+[m[32m    address,[m
[32m+[m[32m    userName,[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst findUserById = (id) => {[m
[32m+[m[32m  return axios.get(`/api/User/${id}`);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst putUpdateUser = (id, name, phone, password, dateOfBirth, address) => {[m
[32m+[m[32m  return axios.put(`/api/User/${id}`, {[m
[32m+[m[32m    name,[m
[32m+[m[32m    phone,[m
[32m+[m[32m    password,[m
[32m+[m[32m    dateOfBirth,[m
[32m+[m[32m    address,[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst deleteUser = (id) => {[m
[32m+[m[32m  return axios.delete(`/api/User/${id}`);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport {[m
[32m+[m[32m  fetchAllUser,[m
[32m+[m[32m  postCreateUser,[m
