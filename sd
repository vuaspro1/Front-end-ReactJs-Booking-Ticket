[32m+[m
[32m+[m[32mconst Pagination = (props) => {[m
[32m+[m[32m  const { totalPages, setPage } = props;[m
[32m+[m[32m  const handlePageClick = (event) => {[m
[32m+[m[32m    setPage(+event.selected + 1);[m
[32m+[m[32m  };[m
[32m+[m[32m  return ([m
[32m+[m[32m    <ReactPaginate[m
[32m+[m[32m      breakLabel="..."[m
