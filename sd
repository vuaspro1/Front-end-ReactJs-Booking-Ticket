[32m+[m[32mimport Button from "react-bootstrap/Button";[m
[32m+[m[32mimport Modal from "react-bootstrap/Modal";[m
[32m+[m[32mimport { deleteUser } from "../../services/UserService";[m
[32m+[m[32mimport { toast } from "react-toastify";[m
[32m+[m
[32m+[m[32mconst ModalConfirm = (props) => {[m
[32m+[m[32m  const { handleClose, show, selectId, handleUpdateTable } = props;[m
[32m+[m
[32m+[m[32m  const handleSave = async () => {[m
[32m+[m[32m    let res = await deleteUser(selectId);[m
[32m+[m[32m    if (res) {[m
[32m+[m[32m      handleClose();[m
[32m+[m[32m      toast.success("Delete successed!");[m
[32m+[m[32m      handleUpdateTable();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      toast.error("Delete false!");[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <Modal backdrop="static" keyboard={false} show={show} onHide={handleClose}>[m
[32m+[m[32m      <Modal.Header closeButton>[m
[32m+[m[32m        <Modal.Title>Delete user</Modal.Title>[m
[32m+[m[32m      </Modal.Header>[m
[32m+[m[32m      <Modal.Body>Are you sure to delete this user?</Modal.Body>[m
[32m+[m[32m      <Modal.Footer>[m
[32m+[m[32m        <Button variant="secondary" onClick={handleClose}>[m
[32m+[m[32m          Close[m
[32m+[m[32m        </Button>[m
[32m+[m[32m        <Button variant="primary" onClick={() => handleSave()}>[m
[32m+[m[32m          Save[m
[32m+[m[32m        </Button>[m
[32m+[m[32m      </Modal.Footer>[m
[32m+[m[32m    </Modal>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default ModalConfirm;[m
