[32m+[m[32m      pageRangeDisplayed={3}[m
[32m+[m[32m      pageCount={totalPages}[m
[32m+[m[32m      previousLabel="< previous"[m
[32m+[m[32m      pageClassName="page-item"[m
[32m+[m[32m      pageLinkClassName="page-link"[m
[32m+[m[32m      previousClassName="page-item"[m
[32m+[m[32m      previousLinkClassName="page-link"[m
[32m+[m[32m      nextClassName="page-item"[m
[32m+[m[32m      nextLinkClassName="page-link"[m
[32m+[m[32m      breakClassName="page-item"[m
[32m+[m[32m      breakLinkClassName="page-link"[m
[32m+[m[32m      containerClassName="pagination"[m
[32m+[m[32m      activeClassName="active"[m
[32m+[m[32m    />[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
