[32m+[m[32mimport { fetchAllRole } from "../../services/RoleService";[m
[32m+[m
[32m+[m[32mconst TableUser = (props) => {[m
[32m+[m[32m  const [listUsers, setListUsers] = useState([]);[m
[32m+[m[32m  const [page, setPage] = useState(0);[m
[32m+[m[32m  const [totalPages, setTotalPages] = useState(0);[m
[32m+[m[32m  const [dataUserEdit, setDataUserEdit] = useState({});[m
[32m+[m[32m  const [listRoles, setListRoles] = useState([]);[m
