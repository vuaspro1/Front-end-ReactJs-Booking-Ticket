[32m+[m[32m  const [isShowModalEdit, setIsShowModalEdit] = useState(false);[m
[32m+[m[32m  const [isShowModalConfirm, setIsShowModalConfirm] = useState(false);[m
[32m+[m[32m  const [selectId, setSelectId] = useState(0);[m
[32m+[m
[32m+[m[32m  const handleClose = () => {[m
[32m+[m[32m    setIsShowModalAdd(false);[m
[32m+[m[32m    setIsShowModalEdit(false);[m
[32m+[m[32m    setIsShowModalConfirm(false);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleDeleteUser = (user) => {[m
[32m+[m[32m    setSelectId(user?.id);[m
[32m+[m[32m    setIsShowModalConfirm(true);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleEditUser = (user) => {[m
[32m+[m[32m    setIsShowModalEdit(true);[m
[32m+[m[32m    setDataUserEdit(user);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleUpdateTable = () => {[m
[32m+[m[32m    getUsers();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    getUsers(page);[m
[32m+[m[32m    getRoles();[m
[32m+[m[32m  }, [page]);[m
[32m+[m
[32m+[m[32m  const getRoles = async (page) => {[m
[32m+[m[32m    let res = await fetchAllRole(page);[m
[32m+[m[32m    if (res && res.data) {[m
[32m+[m[32m      setListRoles(res.data);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const getUsers = async (page) => {[m
[32m+[m[32m    let res = await fetchAllUser(page || 1);[m
[32m+[m[32m    if (res && res.data) {[m
[32m+[m[32m      setListUsers(res.data);[m
[32m+[m[32m      setTotalPages(Math.ceil(res.totalItem / res.pageSize));[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m[32m  return ([m
[32m+[m[32m    <>[m
[32m+[m[32m      <div className="my-3 add-new">[m
[32m+[m[32m        <span>[m
[32m+[m[32m          <b>List User:</b>[m
[32m+[m[32m        </span>[m
[32m+[m[32m        <button[m
[32m+[m[32m          className="btn btn-success"[m
[32m+[m[32m          onClick={() => setIsShowModalAdd(true)}[m
[32m+[m[32m        >[m
[32m+[m[32m          Add[m
[32m+[m[32m        </button>[m
[32m+[m[32m      </div>[m
[32m+[m[32m      <Table striped bordered hover>[m
[32m+[m[32m        <thead>[m
[32m+[m[32m          <tr>[m
[32m+[m[32m            <th>ID</th>[m
[32m+[m[32m            <th>Username</th>[m
[32m+[m[32m            <th>Name</th>[m
[32m+[m[32m            <th>Phone</th>[m
[32m+[m[32m            <th>Address</th>[m
[32m+[m[32m            <th>Date of birth</th>[m
[32m+[m[32m            <th>Role</th>[m
[32m+[m[32m            <th>Action</th>[m
[32m+[m[32m          </tr>[m
[32m+[m[32m        </thead>[m
[32m+[m[32m        <tbody>[m
[32m+[m[32m          {listUsers &&[m
[32m+[m[32m            listUsers.length > 0 &&[m
[32m+[m[32m            listUsers.map((item, index) => {[m
[32m+[m[32m              const formattedDateOfBirth = new Date([m
[32m+[m[32m                item.dateOfBirth[m
[32m+[m[32m              ).toLocaleDateString();[m
[32m+[m[32m              const roleNames =[m
[32m+[m[32m                item.roles && item.roles.length > 0[m
[32m+[m[32m                  ? item.roles.map((role) => role.name).join(", ")[m
[32m+[m[32m                  : "";[m
[32m+[m[32m              return ([m
[32m+[m[32m                <tr key={`users-${index}`}>[m
[32m+[m[32m                  <td>{item.id}</td>[m
[32m+[m[32m                  <td>{item.userName}</td>[m
[32m+[m[32m                  <td>{item.name}</td>[m
[32m+[m[32m                  <td>{item.phone}</td>[m
[32m+[m[32m                  <td>{item.address}</td>[m
[32m+[m[32m                  <td>{formattedDateOfBirth}</td>[m
[32m+[m[32m                  <td>{roleNames}</td>[m
[32m+[m[32m                  <td>[m
[32m+[m[32m                    <button[m
[32m+[m[32m                      className="btn btn-warning mx-3"[m
[32m+[m[32m                      onClick={() => handleEditUser(item)}[m
[32m+[m[32m                    >[m
[32m+[m[32m                      Edit[m
[32m+[m[32m                    </button>[m
[32m+[m[32m                    <button[m
[32m+[m[32m                      className="btn btn-danger"[m
[32m+[m[32m                      onClick={() => handleDeleteUser(item)}[m
[32m+[m[32m                    >[m
[32m+[m[32m                      Delete[m
[32m+[m[32m                    </button>[m
[32m+[m[32m                  </td>[m
[32m+[m[32m                </tr>[m
[32m+[m[32m              );[m
[32m+[m[32m            })}[m
[32m+[m[32m        </tbody>[m
[32m+[m[32m      </Table>[m
[32m+[m[32m      <Pagination[m
[32m+[m[32m        setTotalPages={setTotalPages}[m
[32m+[m[32m        totalPages={totalPages}[m
[32m+[m[32m        setPage={setPage}[m
[32m+[m[32m        page={page}[m
[32m+[m[32m        getUsers={getUsers}[m
[32m+[m[32m      />[m
[32m+[m[32m      <ModalAdd[m
[32m+[m[32m        show={isShowModalAdd}[m
[32m+[m[32m        handleClose={handleClose}[m
[32m+[m[32m        handleUpdateTable={handleUpdateTable}[m
[32m+[m[32m        listRoles={listRoles}[m
[32m+[m[32m      />[m
[32m+[m[32m      <ModalEdit[m
[32m+[m[32m        show={isShowModalEdit}[m
[32m+[m[32m        handleClose={handleClose}[m
