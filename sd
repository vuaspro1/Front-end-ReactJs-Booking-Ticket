[32m+[m[32mimport axios from "axios";[m
[32m+[m
[32m+[m[32mconst instance = axios.create({[m
[32m+[m[32m  baseURL: "http://localhost:81",[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32minstance.interceptors.response.use([m
[32m+[m[32m  function (response) {[m
[32m+[m[32m    return response.data;[m
[32m+[m[32m  },[m
[32m+[m[32m  function (error) {[m
[32m+[m[32m    return Promise.reject(error);[m
[32m+[m[32m  }[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32mexport default instance;[m
